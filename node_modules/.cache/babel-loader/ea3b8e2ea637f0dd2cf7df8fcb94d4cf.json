'water' }\r\n        ],\r\n        type_filter: \"\"\r\n    }\r\n  },\r\n  methods: {\r\n    sendBack: function() {\r\n        this.closeStats();\r\n          window.scrollTo({\r\n            top: 0,\r\n            behavior: 'smooth',\r\n          });\r\n        this.$emit(\"page\", 0);\r\n        this.$emit(\"search\", this.search);\r\n    },\r\n    home: function() {\r\n        this.search = \"\";\r\n        this.type_filter = \"\";\r\n        this.closeStats();\r\n          window.scrollTo({\r\n            top: 0,\r\n            behavior: 'smooth',\r\n          });\r\n        this.$emit(\"search\", this.search);\r\n        this.$emit(\"filter\", this.type_filter);\r\n        this.$emit(\"page\", 0);\r\n    },\r\n    Filter: function() {\r\n        this.closeStats()\r\n          window.scrollTo({\r\n            top: 0,\r\n            behavior: 'smooth',\r\n          });\r\n        this.$emit(\"filter\", this.type_filter);\r\n    },\r\n    closeStats: function(){\r\n        document.getElementById('stats').style.transform=\"translateY(-100%)\";\r\n        setTimeout(function(){\r\n            document.getElementById('stats').style.display=\"none\";\r\n        }, 200);\r\n    }\r\n  },\r\n    filters: {\r\n    capitalize: function (value) {\r\n        if (!value) return ''\r\n        value = value.toString()\r\n        return value.charAt(0).toUpperCase() + value.slice(1)\r\n        }\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style>\r\n    @import \"../assets/top.css\"; \r\n</style>"],"sourceRoot":"src/components"}]}